  <logger name="ServiceStack.Redis.PooledRedisClientManager" additivity="false">
    <level value="ALL" />
    <appender-ref ref="myServiceStackRedisAppender"/>
  </logger>

  <logger name="ServiceStack.Redis.RedisManagerPool" additivity="false">
    <level value="ALL" />
